<header class="center-align">
  <%= link_to "Workdey", dashboard_path, class: "brand-logo" %>
</header>

<div class="row">
  <h3 class="center-align">Artisan Questionnaire</h3>

  <%= form_tag users_responses_path, class: "col s8 offset-s2",
    method: "post", id: "" do %>

    <%= label_tag "response[serv]", "Question" %>
    <%= text_field_tag "response[services]" %>

    <div class="input-field row">
        <button id="choose_skillset" class="btn waves-effect waves-light"submit">Submit
          <i class="material-icons right">send</i>
        </button>
      </div>
  <% end %>
</div>
