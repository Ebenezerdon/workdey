<header class="center-align">
  <%= link_to "Workdey", dashboard_path, class: "brand-logo" %>
</header>

<div class="row">
  <h3 class="center-align">Artisan Questionnaire</h3>
  <h3 class="left-align">Your Questionaire has been successfully saved. An administrator will contact you to schedule your vetting process.</h5>

  <h4 class="left-align">For reference here are the answers you submitted:</h5>
  <% @questions.each do |question| %>
    <p>
      <h5><%= question.question %></h5>
      <h6><%= @response[:response][question.question] %></h6>
    </p>
  <% end %>

</div>


