<%= render 'partials/sidebar_dashboard' %>
<section class="main container">
  <% if @reference.errors.any? %>
   <div id="error_explanation">
    <h3><%= pluralize(@reference.errors.count, "error") %></h3>
    <ul>
      <% @reference.errors.full_messages.each do |message| %>
      <li><%= message %></li>
      <% end %>
    </ul>
   </div>
  <% end %>
  <div class="row">
    <div class="col m12 offset-m1">
      <%= render 'form' %>
    </div>
  </div>
</section>
